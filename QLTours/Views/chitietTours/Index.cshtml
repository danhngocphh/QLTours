@model IEnumerable<QLTours.Models.chitietTour>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create",new {thanhpho = "Đà lạt",idtour = 1 })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.diadiem.ThanhPho)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tour.Ten)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.STTDiaDiem)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.diadiem.ThanhPho)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tour.Ten)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.STTDiaDiem)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
